package Fundamentos02;

public class Multiplos02 {
	public static void main(String[] args) {
		int total;
		int soma5;
		int soma3;

		for(soma3 = 1; soma3<=20; soma3++) {
			if(soma3 % 3 == 0) {
				System.out.println(soma3);
			}if (int soma5 % 5 == 0) {
				System.out.println(soma5);
			}
		}
		total = soma3 + soma5;
		System.out.println(total);
	}

}
